@Test
publicvoidtset02(){
List<Student>c=newArrayList<>();
c.add(newStudent(1,"张三"));
c.add(newStudent(2,"李四"));
c.add(newStudent(3,"王五"));
c.add(newStudent(4,"赵六"));
c.add(newStudent(5,"钱七"));

//从指定位置往前遍历
ListIterator<Student>listIterator=c.listIterator(c.size()-2);//
while(listIterator.hasPrevious()){
Studentprevious=listIterator.previous();
System.out.println(previous.index+previous.name);
}
for(Studentstu:c){
System.out.println(stu.index+stu.name);
}
}
3王五
2李四
1张三
1张三
2李四
3王五
4赵六
5钱七
